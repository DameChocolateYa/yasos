import <standard>
use {scani, stoint};

factorial = (times: int):
    let x: int = times;
    let t: int = times;
    while (t gt 1) do {
        x muling t sub 1;
        dec t;
    }

    ret x;
endfn

start = ():
    "Ponga un num: ";
    let x: int = stoint(scani());
    x = 3;
    let result: int = get factorial<times: x>;
    "El factorial de ese numero es: ", result, cr;
endfn

start<>;

end();
